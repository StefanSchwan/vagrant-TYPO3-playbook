{
  "name": "stefanschwan/ansible-typo3",
  "authors": [
    {
      "name": "Stefan Schwan",
      "email": "stefan.schwan@gmail.com"
    }
  ],
  "config": {
    "github-oauth": {
      "github.com": "cff978b0a5ffd884491996ce62aebd991eee1208"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    },
    {
      "type": "vcs",
      "url": "https://github.com/helhum/typo3_console.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/helhum/dotenv-connector.git"
    }
  ],
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "docroot"
    },
    "helhum/typo3-console": {
      "install-extension-dummy": false
    },
    "helhum/dotenv-connector": {
      "env-dir": "",
      "allow-overrides": true
    }
  },
  "require": {
    "typo3/cms": "^8.7",
    "helhum/typo3-console": "~4.0",
    "helhum/dotenv-connector": "1.1.0",
    "helhum/config-loader": "^0.1"
  }
}
